@model OtoSanayi.Entities.Hizmet
@{
    ViewBag.Title = "DetayGetir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="tr-section theiaStickySidebar" style="padding:40px;">
    <h2 class="center-block"></h2>
    <div class="row">
        <div class="bg-transparent">
            <div class="section-title">
                <h1><span><a href="#">@Model.kategori.HizmetKategoriAdi</a></span></h1>
            </div>
            @Html.Partial("_PartialResimGalerisi", Model.hizmetResimler)

        </div>

        <h4>@Model.HizmetBaslik</h4>
        @Html.Raw(Model.HizmetIcerik)
        
    </div>
</div>
