@using System.Web.Optimization
@using OtoSanayi.WepApp.Models
<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="author" content="Teknik Oto Sanayi Sitesi">
    <meta name="description" content="Teknik Oto Sanayi Sitesi">

    <title>TOSS | Teknik Oto Sanayi Sitesi</title>

  

     @Styles.Render("~/Content/all")

    <link href="~/Content/plugins/blueimp/css/blueimp-gallery.min.css" rel="stylesheet" />

    <!-- font -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Signika+Negative" rel="stylesheet">

    <!-- icons -->
    <link rel="icon" href="~/img//ico/logotoss.ico">
    <link rel="apple-touch-icon" sizes="144x144" href="~/img/ico/logotoss.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/img/ico/logotoss.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/img/ico/logotoss.png">
    <link rel="apple-touch-icon" sizes="57x57" href="~/img/ico/logotoss.png">
    <!-- icons -->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Template Developed By ThemeRegion -->
</head>
<body>

    <div class="main-wrapper">
        <div class="main-wrapper tr-page-top homepage">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-3 tr-sidebar tr-sticky">
                        <div class="theiaStickySidebar">
                            <div class="tr-menu sidebar-menu">
                                <nav class="navbar navbar-default">
                                    <div class="navbar-header">
                                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                                            <span class="sr-only">Toggle navigation</span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                        </button>
                                        <a class="navbar-brand" href="/Anasayfa"><img class="img-responsive" src="~/img/logotoss.png" alt="Logo"></a>
                                    </div><!-- /navbar-header -->

                                    <div class="collapse navbar-collapse" id="navbar-collapse">
                                        <span class="discover">TEKNİK OTO SANAYİ SİTESİ</span>
                                        <ul class="nav navbar-nav">
                                            <li><a href="/Anasayfa"><i class="fa fa-home" aria-hidden="true"></i>Ana Sayfa</a></li>

                                            <li class="dropdown">
                                                <a data-toggle="dropdown" href="#"><i class="fa fa-cubes" aria-hidden="true"></i>KURUMSAL</a>
                                                <ul class="sub-menu">
                                                    @Html.Action("KurumsalKategoriDoldur", "Kurumsal")
                                                </ul>
                                            </li>

                                            <li class="dropdown">
                                                <a data-toggle="dropdown" href="#"><i class="fa fa-car" aria-hidden="true"></i>FİRMALAR</a>
                                                <ul class="sub-menu">
                                                    @Html.Action("FirmaKategoriDoldur", "Firma")
                                                </ul>
                                            </li>
                                            <li><a href="/Duyuru/DuyuruListele"><i class="fa fa-bullhorn" aria-hidden="true"></i>DUYURULAR</a></li>
                                            <li><a href="/Link/LinkListele"><i class="fa fa-link" aria-hidden="true"></i>LİNKLER</a></li>
                                            <li> <a href="/iletişim"><i class="fa fa-map-marker" aria-hidden="true"></i>İLETİŞİM</a></li>

                                        </ul>
                                    </div>
                                </nav><!-- /navbar -->
                            </div><!-- /left-memu -->
                        </div><!-- /.theiaStickySidebar -->
                    </div>
                    <div class="col-sm-9">
                        <div class="tr-topbar clearfix">
                            <div class="topbar-left">
                                <div class="breaking-news">
                                    <a href="/Ilan/IlanListele"> <span># İlanlar</span></a>
                                    <div id="ticker">
                                        @Html.Action("UstIlanGetir", "Ilan")
                                    </div>
                                </div><!-- breaking-news -->
                            </div><!-- /.topbar-left -->
                            <div class="topbar-right">
                                @if (CurrentUser.GetUser() != "default")
                                {
                                    <div class="user">
                                        <div class="user-image">
                                            <img class="img-responsive img-circle" src="~/img/others/user.png" alt="Image">
                                        </div>
                                        <div class="dropdown user-dropdown">
                                            Merhaba,
                                            <a href="#" aria-expanded="true">@CurrentUser.GetUser()<i class="fa fa-caret-down" aria-hidden="true"></i></a>
                                            <ul class="sub-menu text-left">
                                                <li><a href="/Firma/Index" target="_blank">Yönetim</a></li>
                                                <li><a href="/Home/Logout">Çıkış</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                }
                                <div class="searchNlogin">
                                    <ul>
                                        <li class="search-icon"><i class="fa fa-search"></i></li>
                                        <li><a href="#"><i class="fa fa-facebook-official" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>

                                    </ul>
                                    <div class="search">
                                        <form role="form">
                                            <input type="text" class="search-form" autocomplete="off" placeholder="Ara">
                                        </form>
                                    </div> <!--/.search-->
                                </div>
                            </div><!-- /.topbar-right -->
                        </div><!-- /.tr-topbar -->


                        @RenderSection("AnaSlider", required: false)


                        <div class="row">
                            <div class="col-sm-12">
                                @RenderBody()
                            </div>
                        </div><!-- row -->
                    </div>
                </div><!-- /.row -->
            </div><!-- /.container-fulid -->
        </div><!-- /.main-wrapper -->
    </div>
    <footer id="footer">
        <div class="footer-menu footer-style-two">
            <div class="container">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">ANA SAYFA</a></li>
                    <li><a href="/Kurumsal/DetayGetir/BAŞKANIN-MESAJI/1">KURUMSAL</a></li>
                    <li><a href="/Firma/FirmaListele/TÜMÜ/4">FİRMALAR</a></li>
                    <li><a href="/Duyuru/DuyuruListele">DUYURULAR</a></li>
                    <li><a href="/Link/LinkListele">LİNKLER</a></li>
                    <li><a href="/iletişim">İLETİŞİM</a></li>
                </ul>
            </div>
        </div>
        <div class="footer-widgets footer-widgets-two">
            <div class="container">
                <div class="row">
                    <div class="col-sm-3">
                        <div class="widget-left">
                            <div class="widget-left-content">
                                <a href="/Anasayfa"><img class="img-responsive" src="~/img/logotoss.png" alt="Logo"></a>
                                <div class="copyright">
                                    <p>&copy; @DateTime.Now.Year &nbsp;<a href="#">Teknik Oto Sanayi Sitesi.</a> </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="widget  widget-menu-2">
                            <h2>KURUMSAL</h2>
                            <ul>

                                @Html.Action("KurumsalKategoriDoldur1", "Kurumsal")

                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-2 col-md-3">
                        <div class="widget widget-menu-2">
                            <h2>T.O.S.S' DE NELER YAPABİLİRSİNİZ ?</h2>
                            <ul>

                                @Html.Action("FirmaKategoriDoldur", "Firma")

                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-4 col-md-3">
                        <div class="widget widget-menu-2">
                            <h2>Sosyal Medya Hesaplarımız</h2>
                            <ul>
                                <li><a href="#"><i class="fa fa-facebook-official" aria-hidden="true"></i> Facebook</a></li>
                                <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i> Twitter</a></li>
                                <li><a href="#"><i class="fa fa-youtube" aria-hidden="true"></i> Youtube</a></li>
                                <li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i> Instagram</a></li>

                            </ul>
                        </div>
                    </div>
                </div><!-- /.row -->
            </div>
        </div>
    </footer><!-- /#footer -->
    <!-- JS -->
    
    @Scripts.Render("~/scripts/all")
    <script>
        $(document).ready(function () {

            $('.footable').footable();
            $('.footable2').footable();

        });
    </script>

    <script src="~/scripts/Admin/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/scripts/Admin/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/scripts/Admin/inspinia.js"></script>
    <script src="~/scripts/Admin/plugins/blueimp/jquery.blueimp-gallery.min.js"></script>

    @RenderSection("JSdosya",required:false)
   
