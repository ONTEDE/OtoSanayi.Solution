@using OtoSanayi.Entities
@model List<Duyuru>

@{   
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="tr-section theiaStickySidebar" style="padding:40px;">
   
    <div class="row">
       
        @foreach (var item in Model)
        {
            ViewBag.Title = @item.DuyuruBaslik;

            <h4>@item.DuyuruBaslik</h4><br />
            <small>@item.DuyuruTarih.ToShortDateString()</small>
            @Html.Raw(item.DuyuruIcerik)
        }



    </div>
</div>
